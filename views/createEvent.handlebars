      <div class="row">
        <div class="col-md-6 col-md-offset-3">
        <form id="msform">
              <!-- fieldsets -->
              <fieldset>
                  <h2 class="fs-title">Event Details</h2>
                  <!-- <h3 class="fs-subtitle">Tell us something more about you</h3> -->
                  <label for="">Event Title</label>
                    <input type="text" name="eventTitle" placeholder="Event Title" required/>
                  <label for="">Company Name</label>
                    <input type="text" name="cName" placeholder="Company Name" required/>
                  <label for="">Type</label>
                    <select name="type">
                      <option value="recruitment">Recruitment</option>
                      <option value="training">Training</option>
                      <option value="seminar">Seminar</option>
                      <option value="assessment">Assessment</option>
                      <option value="others">Others</option>
                    </select>
                  <label for="">Status</label>
                    <input type="text" name="status" value="Defined" placeholder="Status"/>
                    <input type="button" name="next" class="next action-button" value="Next"/>
              </fieldset>
              <fieldset>
                  <h2 class="fs-title">Attachments</h2>
                  <!-- <h3 class="fs-subtitle">Your presence on the social network</h3> -->
                  <input type="text" name="twitter" placeholder="Twitter"/>

                  <input type="text" name="facebook" placeholder="Facebook"/>
                  
                  <input type="text" name="gplus" placeholder="Google Plus"/>

                  <input type="button" name="previous" class="previous action-button-previous" value="Previous"/>
                  <input type="button" name="next" class="next action-button" value="Next"/>
              </fieldset>
              <fieldset>
                  <h2 class="fs-title">Position Datails</h2>
                  <!-- <h3 class="fs-subtitle">Your presence on the social network</h3> -->
                  <input type="text" name="twitter" placeholder="Twitter"/>
                  <input type="text" name="facebook" placeholder="Facebook"/>
                  <input type="text" name="gplus" placeholder="Google Plus"/>
                  <input type="button" name="previous" class="previous action-button-previous" value="Previous"/>
                  <input type="button" name="next" class="next action-button" value="Next"/>
              </fieldset>
              <fieldset>
                  <h2 class="fs-title">Schedule</h2>
                  <!-- <h3 class="fs-subtitle">Your presence on the social network</h3> -->
                  <input type="text" name="twitter" placeholder="Twitter"/>
                  <input type="text" name="facebook" placeholder="Facebook"/>
                  <input type="text" name="gplus" placeholder="Google Plus"/>
                  <input type="button" name="previous" class="previous action-button-previous" value="Previous"/>
                  <input type="button" name="next" class="next action-button" value="Next"/>
              </fieldset>
              <fieldset>
                  <h2 class="fs-title">Registration Details</h2>
                  <!-- <h3 class="fs-subtitle">Your presence on the social network</h3> -->
