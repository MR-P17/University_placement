<div class="conteudo-box">
    <div class="conteudo-boxInner">
  <table id="example" class="display" width="100%" cellspacing="0">
        <thead>
            <tr>
                <th>Company</th>
                <th>Event Title</th>
                <th>Status</th>
                <th>Drive Schedule</th>
                <th>Courses</th>
                <th>Eligibility</th>
                <th>Registered</th>
                <th>Approved</th>
                <th>Shortlisted</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
          {{#each result}}
            <tr>
                <td>{{this.rollno}}</td>
                <td>{{this.name}}</td>
                <td>B.TECH-{{this.branch}}</td>
                <td>{{this.cpi}}</td>
                <td>{{this.percent_10}}</td>
                <td>{{this.cpi}}</td>
                <td>{{this.percent_12}}</td>
                <td>{{this.cpi}}</td>
            </tr>
              {{/each}}

        </tbody>
    </table>
  </div>
</div>
<script>
/* Custom filtering function which will search data in column four between two values */
$(document).ready(function(){
    $('#example').DataTable();
});
</script>
