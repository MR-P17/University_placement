<div class="conteudo-box">
    <div class="conteudo-titleSection">
        <strong class="conteudo-title">Title section</strong>
    </div>
    <div class="conteudo-boxInner">
      
      <form class="form" method="post" action="/users/submit_personal">
        <h2>Academic Profile</h2>

        <div class="form-group">
          <label for="rollno">Branch:</label>
          <div class="relative">
            <select class="form-control" type="text" required="">
              <option value="CSE">CSE</option>
              <option value="ECE">ECE</option>
              <option value="CCE">CCE</option>
              <option value="ME">ME</option>
              <option value="MME">MME</option>
            </select>
            <i class="fa fa-graduation-cap"></i>
          </div>
        </div>


        <div class="form-group">
          <label for="email">Class 10<sup>th</sup>:</label>
          <div class="typeOfInput">
            <span>Type : </span>
            <input type="radio" name="10thType"> CGPA <input type="radio" name="10thType"> Percentage
          </div>
          <div class="relative">
            <input class="form-control" id="name" type="text" pattern="[0-9]+" required="" autocomplete="" placeholder="Type Your % here">
            <i class="fa fa-user"></i>
          </div>
        </div>
        <div class="form-group">
          <label for="email">Class 12<sup>th</sup>:</label>
          <div class="typeOfInput">
            <span>Type : </span>
            <input type="radio" name="10thType"> CGPA <input type="radio" name="10thType"> Percentage
          </div>
          <div class="relative">
            <input class="form-control" id="name" type="text" pattern="[0-9]+" required="" autocomplete="" placeholder="Type Your % here">
            <i class="fa fa-user"></i>
          </div>
        </div>
        <div class="form-group">
          <label for="email">Samester Wise SPI : </label>
          <div class="semWise">
              <div class="relative">
                <span>Sem 1 :</span><input class="form-control" id="name" type="text" pattern="[0-9]+" required="" autocomplete="" placeholder="Type Your SPI here">
                <span>Sem 2 :</span><input class="form-control" id="name" type="text" pattern="[0-9]+" required="" autocomplete="" placeholder="Type Your SPI here">
                <span>Sem 3 :</span><input class="form-control" id="name" type="text" pattern="[0-9]+" required="" autocomplete="" placeholder="Type Your SPI here">
                <span>Sem 4 :</span><input class="form-control" id="name" type="text" pattern="[0-9]+" required="" autocomplete="" placeholder="Type Your SPI here">
                <span>Sem 5 :</span><input class="form-control" id="name" type="text" pattern="[0-9]+" required="" autocomplete="" placeholder="Type Your SPI here">
                <span>Sem 6 :</span><input class="form-control" id="name" type="text" pattern="[0-9]+" required="" autocomplete="" placeholder="Type Your SPI here">
              </div>
            </div>
        </div>
        <div class="form-group">
          <label for="email">Current CPI : </label>
          <div class="relative">
            <input class="form-control" id="name" type="text" pattern="[0-9]+" required="" autocomplete="" placeholder="Type Your CPI here">
            <i class="fa fa-user"></i>
          </div>
        </div>
        <div class="form-group">
          <h3>Your Skills</h3>
          <label for="email">Add New Skill  : </label>
          <div class="relative">
            <input class="form-control" id="name" type="text" pattern="[0-9]+" required="" autocomplete="" placeholder="Type Your CPI here">
            <i class="fa fa-plus"></i>
          </div>
        </div>


        <div class="form-group">
          <label for="email">Upload New Resume:</label>
          <div class="relative">
            <div class="input-group">
              <label class="input-group-btn">
                <span class="btn btn-default">
                    Browse&hellip; <input type="file" style="display: none;" multiple>
                </span>
              </label>
              <input type="text" class="form-control" required="" placeholder="Attachment..." readonly>
              <i class="fa fa-link"></i>
            </div>
          </div>
        </div>

        <div class="tright">
          <a href=""><button class="movebtn movebtnre" type="Reset"><i class="fa fa-fw fa-refresh"></i> Reset </button></a>
          <a href=""><button class="movebtn movebtnsu" type="Submit">Update <i class="fa fa-fw fa-paper-plane"></i></button></a>
        </div>
      </form>

    </div>
</div>
